// code to find quadrants
n1=0
n2=0
if n1>0 and n2>0:
    print("first quadrant")
elif n1<0 and n2>0:
    print("second quadrant")
elif n1<0 and n2<0:
    print("third quadrant")
elif n1>0 and n2<0:
    print("fourth quadrant")
elif n1==0 and n2==0:
    print("origin")
elif n1==0 and n2>0:
    print("on y axis")
elif n1>0 and n2==0:
    print("on x axis")
